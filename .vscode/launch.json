    "version": "0.2.0",
    "configurations": [

        {
            "name": "gdb Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/sfhss-study.elf",
            "cwd": "${workspaceFolder}",
            //"stopAtEntry": true,
            "MIMode": "gdb",
            "miDebuggerPath": "arm-none-eabi-gdb",
            "externalConsole": false,
            "preLaunchTask": "Build",
            "debugServerPath": "${workspaceFolder}/tools/debugserver",
            "debugServerArgs": "rosalind 3333 4444 ${workspaceFolder}/build/sfhss-study.elf",
            "serverStarted": "END",
            "miDebuggerServerAddress": "rosalind:3333"
        }
    ]
}